{
  "name": "helloworld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --main-activity com.zoontek.rnbootsplash.RNBootSplashActivity",
    "androidBoot": "react-native run-android --main-activity com.zoontek.rnbootsplash.RNBootSplashActivity",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start2": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "regenerate-bootsplash": "react-native generate-bootsplash src/assets/images/bootsplash_logo.png --background-color=ffffff --logo-width=200 --assets-path=src/assets/images",
    "postversion": "react-native-version",
    "pod": "npx pod-install",
    "rmpod": "cd ios && rm Pods && rm Podfile.lock && pod install && cd ..",
    "a": "npx react-native run-android",
    "a2": "npx react-native run-android  --deviceId emulator-5556",
    "i": "npx react-native run-ios --simulator=\"iPhone 11\"",
    "t": "react-devtools",
    "r": "npx jetify && cd android && ./gradlew assembleRelease",
    "build:apk": "npx jetify && cd android && ./gradlew assembleRelease",
    "build:aab": "npx jetify && cd android && ./gradlew bundleRelease",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-redux": "^7.1.18",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-bootsplash": "^3.2.3",
    "react-native-device-info": "^8.1.5",
    "react-native-flash-message": "^0.1.23",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-global-props": "^1.1.5",
    "react-native-immersive-bars": "1.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-modal": "^12.0.3",
    "react-native-pager-view": "^5.2.1",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.2.0",
    "react-native-tab-view": "^3.0.1",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.10",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "17.0.1",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
